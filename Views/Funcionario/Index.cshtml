@model IEnumerable<Conclusao_DisciplinarMvc.Models.FuncionarioViewModel>

For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
@{
    ViewBag.Title = "Funcionarios";
}

<h2>Relação de Funcionários</h2>

<table class="table">
    <tr><!--Títulos das colunas da tabela-->
        <th>@Html.DisplayNameFor(model => model.Id)</th>
        <th>@Html.DisplayNameFor(model => model.Nome)</th>
        <th>@Html.DisplayNameFor(model => model.Funcao)</th>
        <th>@Html.DisplayNameFor(model => model.HorasDeServico)</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.Id)</td>
            <td>@Html.DisplayFor(modelItem => item.Nome)</td>
            <td>@Html.DisplayFor(modelItem => item.Funcao)</td>
            <td>@Html.DisplayFor(modelItem => item.HorasDeServico)</td>

            <td><!--Coluna para Links/botões-->
        </tr>
    }
</table>